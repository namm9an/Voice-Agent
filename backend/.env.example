# Copy to .env and fill in your real values

# LLM (OpenAI-compatible chat completions) - microsoft/Phi-3.5-mini-instruct
LLM_API_KEY=
LLM_BASE_URL=
LLM_MODEL=microsoft/Phi-3.5-mini-instruct

# Whisper Turbo (OpenAI-compatible audio transcriptions)
WHISPER_API_KEY=
WHISPER_BASE_URL=
WHISPER_MODEL=openai/whisper-large-v3-turbo

# If you use a single token for both services, set both *_API_KEY to that token

# Parler TTS (primary) and XTTS (fallback)
PARLER_TTS_BASE_URL=http://164.52.192.118:8001
XTTS_TTS_BASE_URL=
TTS_VOICE=default
TTS_LANGUAGE=en
TTS_MODEL=parler-tts/parler-tts-mini-v1

# General
CORS_ORIGINS=http://localhost:3000
AUDIO_SAMPLE_RATE=16000
AUDIO_MAX_DURATION_SECONDS=30
MAX_AUDIO_SIZE_MB=10

# Optional OpenAI-style default key if you have one
OPENAI_API_KEY=

